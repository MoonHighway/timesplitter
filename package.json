{
  "name": "@moonhighway/timesplitter",
  "version": "0.0.6",
  "description": "⏰ A web application that helps presenters run great courses that stay on track and cover everything.",
  "keywords": [
    "Instructor led, presenter, classroom, training, tools, timers, notes"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoonHighway/timesplitter.git"
  },
  "dependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "express": "^4.17.1",
    "react-select": "^4.3.1"
  },
  "bin": {
    "timesplitter": "server/index.js"
  },
  "scripts": {
    "develop": "timesplitter-dev ./DevelopmentSampleContent",
    "start:server": "nodemon ./server ./DevelopmentSampleContent",
    "start:client": "react-scripts start",
    "start": "export NODE_ENV=development; concurrently \"npm run start:server\" \"npm run start:client\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "bump": "node bump"
  },
  "proxy": "http://localhost:4224",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mdx-js/runtime": "^1.6.22",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "concurrently": "^6.0.0",
    "mdx-loader": "^3.0.2",
    "nodemon": "^2.0.7",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1"
  }
}
